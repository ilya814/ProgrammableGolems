{
  "schemaVersion": 1,
  "id": "programmablegolem",
  "version": "${version}",
  "name": "Programmable Iron Golems",
  "description": "Upgrade Iron Golems with brains and program them to work for you!",
  "authors": ["ilya814"],
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.programmablegolem.ProgrammableGolemMod"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.programmablegolem.ProgrammableGolemClient"
      }
    ]
  },
  "mixins": [
    "programmablegolem.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-api": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "~1.21.1"
  }
}
